{
  "name": "llm-eval-runner",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsgo -p tsconfig.json",
    "start": "cd web && bun run start",
    "deploy:build": "cd web && bun install && cd app && bun install && bun run build",
    "deploy:start": "cd web && bun run start",
    "typecheck": "tsgo --noEmit",
    "lint": "cd web && bun run lint && cd ../web/app && bun run lint",
    "format": "cd web && bun run format && cd ../web/app && bun run format",
    "clean": "rm -rf dist",
    "dev": "bun run tsx scripts/dev.ts",
    "cli:run": "bun run tsx src/cli/run.ts",
    "cli:aggregate": "bun run tsx src/cli/aggregate.ts",
    "cli:extract": "bun run tsx src/cli/extract-failures.ts",
    "cli:run-one": "bun run tsx src/cli/run-one.ts",
    "promptfoo:eval": "promptfoo eval -c promptfoo/promptfooconfig.yaml",
    "promptfoo:view": "promptfoo view --yes"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "private": true,
  "devDependencies": {
    "@types/node": "^25.0.7",
    "@typescript/native-preview": "^7.0.0-dev.20260117.1",
    "promptfoo": "^0.120.14",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "date-fns": "^4.1.0",
    "fast-json-stable-stringify": "^2.1.0",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "nanoid": "^5.1.6",
    "p-limit": "^7.2.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  }
}
